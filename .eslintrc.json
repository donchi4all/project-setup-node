{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "warn",
        "@typescript-eslint/no-unused-vars": [
            "warn"
        ],
        "camelcase": [
            "error",
            {
                "properties": "always"
            }
        ]
    }
}