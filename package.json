{
  "name": "api-template",
  "version": "3.0.0",
  "description": "Nodejs-api-template",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/index",
    "dev": "npm run build-routes && nodemon index.ts",
    "build": "npm run build-routes && tsc",
    "build-routes": "tsoa spec-and-routes",
    "test": "jest --detectOpenHandles",
    "seed": "./node_modules/.bin/sequelize db:seed:all",
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "lint": "eslint 'src/**/*.{ts,js}' --fix",
    "format": "prettier --write 'src/**/*.{ts,js,json,md}'"
  },
  "lint-staged": {
    "src/**/*.{ts,js,json,md}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": "git+https://github.com/fracxn/api.git",
  "keywords": [
    "expense",
    "api",
    "portal"
  ],
  "author": "Team Cairo",
  "license": "ISC",
  "bugs": {
    "url": "https://bitbucket.org/vfddevteam/nodejs-api-template/src/master-v2"
  },
  "homepage": "https://bitbucket.org/vfddevteam/nodejs-api-template#readme",
  "dependencies": {
    "audit-pro": "^1.2.2",
    "axios": "^1.8.1",
    "body-parser": "^1.20.3",
    "cookie": "^1.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cron-decorators": "^0.1.5",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "errorhandler": "^1.5.1",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "gulp": "^5.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.12.0",
    "pluralize": "^8.0.0",
    "puppeteer": "^24.3.0",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6",
    "swagger-ui-express": "^5.0.1",
    "tsoa": "^6.6.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/eslint": "^9.6.1",
    "@types/express": "^5.0.0",
    "@types/got": "^9.6.12",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.8",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.12.2",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.2",
    "nodemon": "^3.1.9",
    "plop": "^4.0.1",
    "prettier": "^3.6.2",
    "sequelize-cli": "^6.6.2",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
