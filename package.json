{
  "name": "donsoft-api",
  "version": "1.0.0",
  "description": "donsoft api tokenization project",
  "main": "index.ts",
  "scripts": {
    "start": "npm run build-routes && nodemon index.ts",
    "build": "npm run build-routes && tsc && npm run lint",
    "build-routes": "tsoa spec-and-routes",
    "postinstall": "cp -R node_modules_patches/ node_modules/",
    "test": "jest --forceExit --coverage --verbose",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "db-migrate": "node ./node_modules/sequelize-cli-typescript/lib/sequelize db:migrate",
    "db-seed": "node ./node_modules/sequelize-cli-typescript/lib/sequelize db:seed:all"
  },
  "repository": "git+https://github.com/fracxn/wallet.git",
  "keywords": [
    "tokenization",
    "api",
    "wallet"
  ],
  "author": "Mkparu Chisom Emmanuel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fracxn/wallet/issues"
  },
  "homepage": "https://github.com/fracxn/wallet#readme",
  "dependencies": {
    "accesscontrol": "^2.2.1",
    "body-parser": "^1.20.1",
    "crypto-random-string": "^5.0.0",
    "dotenv": "^16.0.3",
    "errorhandler": "^1.5.1",
    "express": "^4.18.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.25.8",
    "sequelize-typescript": "^2.1.5",
    "swagger-ui-express": "^4.6.0",
    "ts-node-dev": "^2.0.0",
    "tsoa": "^4.1.3",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.10",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.10",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "sequelize-cli-typescript": "^3.2.0-c",
    "sequelize-typescript-migration": "^0.0.1-beta.3",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
